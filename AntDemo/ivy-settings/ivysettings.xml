<ivysettings>
	<settings defaultResolver="dual-resolver" />
	<resolvers>
		<dual name="dual-resolver">
			<filesystem name="ivys">
				<ivy pattern="${ivy.settings.dir}/repository/[module]-ivy-[revision].xml" />
			</filesystem>
			<ibiblio name="ibiblio" m2compatible="true" usepoms="false"/>
		</dual>
	</resolvers>
</ivysettings>
